	" insert two columns": {
		"prefix": "columns",
		"body": [
		  "\\begin{frame}",
		  "    \\frametitle{}",
		  "\t\\begin{columns}",
		  "\t\t\\begin{column}[t]{0.46\\linewidth}",
		  "\t\t\t$1",
		  "\t\t\t\\begin{itemize}",
		  "\t\t\t\t\\Item $3 ",
		  "\t\t\t\t\\Item $4",
		  "\t\t\t\\end{itemize}",
		  "\t\t\\end{column}",
		  "\t\t\\begin{column}[t]{0.46\\linewidth}",
		  "\t\t\t$2",
		  "\t\t\t\\vspace{1ex}",
		  "\t\t\t\\begin{itemize}",
		  "\t\t\t\t\\Item  $5",
		  "\t\t\t\t\\Item  $6",
		  "\t\t\t\\end{itemize}",
		  "\t\t\\end{column}",
		  "\t\\end{columns}",
		  "\\end{frame}"
		],
		"description": "insert two columns"
	  },

	"insert figure": {
		"prefix": "figure",
		"body": [
		  "\\begin{frame}",
		  "    \\frametitle{}",
		  "  \\begin{center}",
		  "  \t \\includegraphics[width=0.8\\textwidth,height=2.5in]{figs/$1.png}",
		  "  \\end{center}",
		  "\\end{frame}"
		],
		"description": "insert figure"
	  },

	  "insert two subfigures": {
		"prefix": "subfigure",
		"body": [
		  "\\begin{frame}",
		  "    \\frametitle{}",
		  "    \\begin{figure}",
		  "        \\centering",
		  "        \\subfigure[]{\\includegraphics[width=$1 cm]{figs/$3.png}}",
		  "        \\subfigure[]{\\includegraphics[width=$2 cm]{figs/$4.png}}",
		  "        %\\caption{} %图片标题",
		  "        %\\label{fig:1}  ",
		  "    \\end{figure} ",
		  "    \\setcounter{subfigure}{0}",
		  "\\end{frame}"
		],
		"description": "insert two subfigures"
	  },
	
	  "insert text figure mixed": {
		"prefix": "cutwin",
		"body": [
		  "\\opencutright ",
		  "\\renewcommand{\\windowpagestuff}{\\flushright ",
		  "\\quad \\includegraphics[width=3cm]{figs/$1.png}}",
		  "\t\\begin{cutout} {2}{2.2cm}{6.2cm}{6}",
		  "$2 text",
		  "\t\\end{cutout} "
		],
		"description": ""
	  },

	"insert quotation": {
		"prefix": "quotation",
		"body": [
		  "\\begin{frame} ",
		  "    \\begin{quotation}",
		  "        \"在近代物理学结出硕果的那些重大问题中，很难找到一个问题是爱因斯坦没有做出过重要贡献的。",
		  "        在他的各种推测中，他有时可能也曾经没有中标的，例如他的光量子假设，就有点迷失了方向\\dots\"  \\\\",
		  "        \\rightline{$\\cdots$ 普朗克\\hspace{3em}}   ",
		  "    \\end{quotation}",
		  "\\end{frame}"
		],
		"description": "insert quotation"
	  },
	
