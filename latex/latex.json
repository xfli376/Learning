    			"\\label{tab:${3:label}}",
    			"\\begin{tabular}{${5:c}}",
    			"\\$0${5/((?<=.)c|l|r)|./(?1: & )/g}",
    			"\\end{tabular}",
				"\\end{table}"
		],
		"description": "Insert table"
	},

	"mean1": {
