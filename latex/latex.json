{
	// Place your snippets for latex here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"insert main file": {
		"prefix": "main-beamer",
		"body": [
		  "\\documentclass[12pt,UTF8,aspectratio=169]{beamer} ",
		  "%aspectratio=1610：160mm，100mm",
		  "%aspectratio=169：160mm，90mm",
		  "%aspectratio=43：128mm，96mm，默认尺寸",
		  "\\RequirePackage{mybeamer}",
		  "%%% sudo vim /usr/local/texlive/2021/texmf-dist/tex/xelatex/mybeamer",
		  "%%%% sudo texhash",
		  "",
		  "%-------------------正文-------------------------%",
		  "%                                               %",
		  "%                                               %",
		  "\\begin{document}                                %",
		  "%                                               %",
		  "%                                               %",
		  "%-----------------------------------------------%",
		  "",
		  "%题目，作者，学校，日期                ",
		  "\\author{李小飞}",
		  "\\title{\\textbf{\\Huge 量子力学与统计物理}}",
		  "\\subtitle{Quantum mechanics and statistical physics}",
		  "\\institute[电子科技大学]{光电科学与工程学院}",
		  "\\date{\\today}",
		  "",
		  "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
		  "%\\frame[plain]{\\titlepage}",
		  "\\begin{frame} [plain]",
		  "    \\setbeamertemplate{headline} {} %",
		  "    \\Background[17] ",
		  "    \\maketitle",
		  "    \\addtocounter{framenumber}{-1} ",
		  "\\end{frame}",
		  "%\\maketitle",
		  "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
		  "",
		  "%-----------章节-------------------------%",
		  "%\\include{chapter1.tex}",
		  "%\\include{lecture1.tex}",
		  "%--------------------------------------%",
		  "",
		  "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
		  "",
		  "\\begin{frame}[plain] ",
		  "    \\Background[2] ",
		  "\\begin{center}",
		  "{\\huge \\color{deepred} \\textrm{Thanks for your attention!  \\\\ \\vspace{1.0em}",
		  "         A \\& Q}}",
		  "\\end{center}",
		  "    \\addtocounter{framenumber}{-1} ",
		  "\\end{frame}",
		  "",
		  "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
		  "\\end{document}",
		  "%---------------THE END-----------------%"
		],
		"description": "insert main file for beamer"
	  },

	"insert introduction of a lecture": {
		"prefix": "introduction",
		"body": [
		  "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
		  "\\section{1.课程简介}",
		  "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
		  "",
		  "\\begin{frame}",
		  "    \\frametitle{课程目标}",
		  "        \\begin{enumerate}",
		  "            \\Item $1",
		  "        \\end{enumerate}",
		  "\\end{frame}",
		  "\\begin{frame} ",
		  "    \\frametitle{分数构成}",
		  "        \\begin{enumerate}",
		  "            \\Item Normal results $2\\%",
		  "            \\Item Midterm examination results $3\\%",
		  "            \\Item Final examination results $4\\%",
		  "        \\end{enumerate}",
		  "\\end{frame}",
		  "",
		  "\\begin{frame} ",
		  "    \\frametitle{教学效果}",
		  "    \\centering",
		  "    \\includegraphics[width=1.0\\textwidth,height=5.0cm]{figs/$5.png}",
		  "\\end{frame}",
		  "",
		  "\\begin{frame}",
		  "    \\frametitle{参考书目}",
		  "        \\begin{itemize}",
		  "            \\Item 《量子力学》卷I，II， 曾谨言， 科学出版社， 2008           ",
		  "            \\Item Principles of quantum mechanics, shankar",
		  "            \\Item Modern quantum mechanics, shankar",
		  "            \\Item Lectures on quantum mechanics, weinberg",
		  "            \\Item Principles of quantum mechanics, Dirac",
		  "        \\end{itemize}",
		  "\\end{frame}",
		  "",
		  "\\begin{frame}",
		  "    \\begin{tcolorbox4}[三条军规]",
		  "        \\begin{enumerate}",
		  "            \\Item Objects are wave-particles and can be in states of superposition",
		  "            \\Item Rule 1 holds as long as you don't measure",
		  "            \\Item Measurement gives random results",
		  "        \\end{enumerate}",
		  "    \\end{tcolorbox4} ",
		  "\\end{frame}"
		],
		"description": "insert introduction of a lecture"
	  },

	"insert chapter-n": {
		"prefix": ["chapter-n", "\\chapter-n"],
		"body": [
		  "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
		  "\\begin{frame} [plain]",
		  "    \\frametitle{}",
		  "    \\Background[1] ",
		  "    \\begin{center}",
		  "    { {\\huge 第$1章：$2}}",
		  "    \\end{center}  ",
		  "    \\addtocounter{framenumber}{-1}   ",
		  "\\end{frame}",
		  "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%"
		],
		"description": "insert chapter-n for beamer"
	  },
	
	"insert lecture-n": {
		"prefix": ["lecture-n","\\lecture"],
		"body": [
		  "\\begin{frame} [plain]",
		  "  \\frametitle{}",
		  "  \\Background[1] ",
		  "  \\begin{center}",
		  "  { {\\huge 第$1讲：$2 }}",
		  "  \\end{center}  ",
		  "  \\addtocounter{framenumber}{-1}   ",
		  "\\end{frame}",
		  "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%"
		],
		"description": "insert lecture n for beamer "
	  },
	
	  "insert review": {
		"prefix": ["review", "\\前情回顾"],
		"body": [
		  "\\begin{frame}",
		  "    \\frametitle{}",
		  "    \\begin{tcolorbox3}[前情回顾]",
		  "\t \\begin{itemize}",
		  "       \t\t \\item $1",
		  "    \t \\end{itemize}",
		  "    \\end{tcolorbox3}",
		  "\\end{frame}"
		],
		"description": "insert review for previous lecture"
	  },
	  
	"insert theorem": {
		"prefix": ["theorem","\\原理"],
		"body": [
		  "\\begin{frame} ",
		  "    \\begin{tcolorbox4}[$1]",
		  "    $2",
		  "    \\end{tcolorbox4}",
		  "\\end{frame}",
		  ""
		],
		"description": "insert theorem"
	  },
	
	"insert definition ": {
		"prefix": ["definition","\\定义"],
		"body": [
		  "    \\begin{tcolorbox2}[0.86]{定义}",
		  "      $1: $0",
		  "    \\end{tcolorbox2}"
		],
		"description": ""
	  },
	
	"insert example": {
		"prefix": ["example", "\\例题"],
		"body": [
		  "\\begin{frame}",
		  "    \\frametitle{}",
		  "  \\例[$1.求解如下方程]{",
		  "   \\[$2\\]",
		  "}",
		  "  \\解 ~ $0",
		  " ",
		  "\\end{frame}",
		],
		"description": "insert example"
	  },
	
	"insert proof": {
		"prefix": ["proof", "\\证明题"],
		"body": [
		  "\\begin{frame}",
		  "    \\frametitle{}",
		  "  \\例[$1.试证明]{",
		  "   \\[$2\\]",
		  "}",
		  "  \\证 ~ $0",
		  " ",
		  "\\end{frame}",
		],
		"description": "insert proof"
	  },
	
	  "insert exampleblock": {
		"prefix": "exampleblock",
		"body": [
		  "\\begin{frame}",
		  "\\begin{exampleblock} {例 13、求二阶齐次常微分方程的通解}",
		  "\t$1",
		  "\t\\end{exampleblock}",
		  "\t\\alert{解、}  $2",
		  "\\end{frame}"
		],
		"description": "insert exampleblock"
	  },

	"insert seatwork": {
