	// for math

	"math": {
		"prefix": ["math","cmath"],
		"body": [
			"\\[  ",
			" \t $1 ",
			" \\] $0",
		],
		"description": "central math"
	},

	"mathin": {
		"prefix": "inmath",
		"body": [
		  "$ $1 $ $0"
		],
		"description": "inlinemath"
	},

	"insert left cases": {
		"prefix": ["cases", "lcases"],
		"body": [
		  "\\[ \\begin{cases}",
		  "\t\tu_{t} =u_{xx} ~~,~~ 0<x<l, t>0\\\\",
		  "\t\tu_x (0,t) =0, u_x (l,t)=0 \\\\",
		  "\t\tu(x,0) =\\psi(x)",
		  "\\end{cases}$ \\]"
		],
		"description": "insert left cases"
	  },
	
	"insert right cases": {
			"prefix": "rcases",
			"body": [
			  "\\[\\left.",
			  "\\begin{array}{rrr}",
			  "\t $1 & $2\\\\",
			  "\t $3 & $4 ",
			  "\\end{array}\right}\\]"
			],
			"description": ""
		},

	"insert align*": {
		"prefix": "aligned",
		"body": [
		  "    \\[ \\begin{align*}",
		  "         $1 &=$2\\\\",
		  "            &=$3",
		  "    \\end{align*}\\]"
		],
		"description": "insert  aligned"
	  },

	  "insert  expansion": {
        "prefix": ["expansion", "\\sum"],
		"body": [
			"\\[\\sum_{$1: n=1} ^{$2: \\infty} {$3: a_n} {$4: \\varphi_n}"
		],
		"description": "Insert a expansion equation."
    },
	
	// END Math 

	"Bold Text": {
        "prefix": ["bold", "\\bold", "**"],
        "body": "\\textbf{$1}$0",
        "description": "Insert bold text."
    },

	"Itemize": {
