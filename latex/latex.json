        "body": [
            "\\begin{itemize}",
            "\t\\item $1",
            "\\end{itemize}$0"
        ]
    },
	"Insert enumerate":{
		"prefix": "enumerate",
		"body":["\\begin{enumerate}\n\t\\item ${1:enumerate1}\n\t\\item ${2:enumerate2}\n\\end{enumerate}"],
